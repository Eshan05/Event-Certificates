    <aside id="links" class="absolute"></aside>
  </body>
  <script>
    const searchInput = document.getElementById('search-input');
    const links = document.getElementById('linkwrap');
    searchInput.addEventListener('input', () => {
      const searchTerm = searchInput.value.toLowerCase();
      const items = links.getElementsByTagName('a');

      for (let item of items) {
        const textContent = item.textContent.toLowerCase();
        if (textContent.includes(searchTerm)) {
          item.style.display = ''; // Show item
        } else {
          item.style.display = 'none'; // Hide item
        }
      }
    });

  </script>
  <script src="util.js"></script>
  <script>
    const headerHTML = createHeader("Certificate", "Your organization", "images/ACES_Logo.png");
    document.getElementsByTagName('header')[0].innerHTML = headerHTML
  </script>

